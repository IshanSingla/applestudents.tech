{
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "app.js",
          "headers": {
            "content-security-policy-report-only": "font-src * 'unsafe-inline';"
          }
        }
    ]
  }