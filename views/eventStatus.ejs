<!DOCTYPE html>
<html lang="en">
  <head>
    <link rel="icon" href="/assets/ieee.png" />
    <link rel="stylesheet" href="/css/styles.css" />
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css"
      integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm"
      crossorigin="anonymous"
    />
    <!-- <meta http-equiv="Content-Security-Policy" content="default-src 'none'; connect-src 'self';font-src 'self'; img-src 'self' data: https:; style-src 'self' ; script-src 'self'"> -->
    <title>Document</title>
  </head>
  <body>
    <section class="topFrontSection">
      <div class="navLogo" href="/">
        <div>
          <img src="/assets/ieee.png" class="logo" alt="" />
        </div>
      </div>
      <div class="evnt-status">
        <% if( Status ) { %>
        <h1>App is not ready for <%= OsName %></h1>
        <!-- <br> -->
        <a href="/<%=evntdata.route%>/form"
          ><button class="btn btn-outline-danger btn-lg">
            Continue with Browser
          </button></a
        >
        <% } %> <% if(! Status ) { %>
        <h1>Form Closed</h1>
        <%} %>
      </div>
    </section>
  </body>
  <script src="socket.io/socket.io.js"></script>
  <script>
    const socket = io.connect('/');
    socket.on('connect', function() {
      console.log('connected');
    socket.on("reconnect_error", (error) => {
     socket.disconnect()
   })
   socket.on("chat", (payload) => {
      window.location.reload()
    });
    });

  </script>
</html>
