<!DOCTYPE html>
<html lang="en">
  <head>
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css"
    />
    <link rel="icon" href="assets/ieee.png" />
    <link rel="stylesheet" href="/css/styles.css">
    <title>Document</title>
  </head>
  <body>
    <!-- NAVBAR SECTION -->
    <nav class="navbar">
      <ul>
        <li>
          <a href="index.html">
            <img src="assets/ieee.png" class="logo-image" alt="logo" />
          </a>
        </li>
        <li><a href="/" class="nav-item">Home</a></li>
        <li><a href="/events" class="nav-item">Events</a></li>
        <li><a href="/about" class="nav-item">About</a></li>
      </ul>
    </nav>

    <!-- Top Navigation Menu -->
    <div class="topnav">
      <span href="/" class="active"
        ><img src="assets/ieee.png" alt=""
      /></span>
      <!-- Navigation links (hidden by default) -->
      <div id="myLinks">
        <a href="/" class="nav-item">Home</a>
        <a href="/events" class="nav-item">Events</a>
        <a href="/about" class="nav-item">About</a>
      </div>
      <!-- "Hamburger menu" / "Bar icon" to toggle the navigation links -->
      <a href="javascript:void(0);" class="icon" onclick="myFunction()">
        <i class="fa fa-bars"></i>
      </a>
    </div>
    <script>
      function myFunction() {
        var x = document.getElementById("myLinks");
        if (x.style.display === "block") {
          x.style.display = "none";
        } else {
          x.style.display = "block";
        }
      }
    </script>

    
